{
  "name": "cssbattle",
  "version": "1.0.0",
  "source": "src/index.html",
  "author": "https://github.com/felipeog",
  "license": "ISC",
  "scripts": {
    "build": "parcel build",
    "download-targets-images": "ts-node ./scripts/downloadTargetsImages.ts",
    "generate-all-solutions-images": "ts-node ./scripts/generateAllSolutionsImages.ts",
    "generate-empty-targets": "ts-node ./scripts/generateEmptyTargets.ts",
    "generate-readme": "ts-node ./scripts/generateReadme.ts",
    "generate-targets-export": "ts-node ./scripts/generateTargetsExport.ts",
    "jest": "jest",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "scrape-targets-info": "ts-node ./scripts/scrapeTargetsInformation.ts",
    "start": "parcel --no-cache --open",
    "test": "npm run generate-all-solutions-images && jest",
    "ts-node": "ts-node",
    "update-github-issues": "npm run generate-all-solutions-images && ts-node ./scripts/updateGithubIssues.ts"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/pixelmatch": "^5.2.4",
    "@types/pngjs": "^6.0.1",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "axios": "^0.26.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.8.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.3",
    "node-html-to-image": "^4.0.0",
    "octokit": "^3.1.0",
    "parcel": "^2.2.1",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "pixelmatch": "^5.2.1",
    "pngjs": "^6.0.0",
    "postcss": "^8.4.6",
    "postcss-modules": "^4.3.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.6.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{html,css}": [
      "prettier --write"
    ]
  },
  "browserslist": "> 0.5%, last 2 versions, not dead"
}
